<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kh.test.mapper.StudentMapper">
  
  <select id="selectAllList" resultType="Student">
  	SELECT STUDENT_NO, NAME, ADDRESS, AGE
  	FROM "STUDENT"
  	ORDER BY STUDENT_NO ASC
  </select>
  
</mapper>
